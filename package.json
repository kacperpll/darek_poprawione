{
  "name": "es6-boilerplate",
  "version": "1.0.0",
  "description": "Standard ES6 Boilerplate configured with babel and webpack",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server",
    "dev:server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "build": "webpack -p"
  },
  "keywords": [
    "es6"
  ],
  "author": "metagrover",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "uuid-v4": "^0.1.0"
  }
}
